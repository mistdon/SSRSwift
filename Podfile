target 'SSRSwift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SSRSwift
  pod 'SwiftyJSON'

  target 'SSRSwiftTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SSRSwiftUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
